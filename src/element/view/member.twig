<div class="row">
  <div class="col">
    <div class="form-group">
      <label for="text" class="control-label">会員番号</label>
      <input type="text" name="text" id="text" value="000001" class="form-control" readonly>
    </div>
    <div class="form-group">
      <label for="text" class="control-label">名前</label>
      <input type="text" name="text" id="text" value="" class="form-control" readonly>
    </div>
    <div class="form-group">
      <label for="text" class="control-label">フリガナ</label>
      <input type="text" name="text" id="text" value="" class="form-control" readonly>
    </div>
    <div class="form-group">
      <label for="text" class="control-label">電話番号</label>
      <input type="tel" name="tel" id="text" value="" class="form-control" readonly>
    </div>
    <div class="form-group">
      <label for="text" class="control-label">メールアドレス</label>
      <input type="email" name="email" id="text" value="" class="form-control" readonly>
    </div>
    <div class="form-group">
      <label for="birthday" class="control-label">生年月日</label>
      <input type="date" name="birthday" id="birthday" value="" class="form-control"
        pattern="[0-9]\{4}-[0-9]\{2}-[0-9]\{2}" readonly>
    </div>
    <!-- /.form-group -->
  </div>
  <div class="col">
    <div class="row">
      <div class="col">
        <div class="form-group">
          <label for="radio" class="control-label">ヒットポイント(HP)</label>
          <div>
            <div class="form-check form-check-inline">
              <input type="radio" name="hp" value="a" id="hp-a" class="form-check-input" disabled>
              <label class="form-check-label" for="hp-a">A</label>
            </div>
            <div class="form-check form-check-inline">
              <input type="radio" name="hp" value="b" id="hp-b" class="form-check-input" disabled>
              <label class="form-check-label" for="hp-b">B</label>
            </div>
            <div class="form-check form-check-inline">
              <input type="radio" name="hp" value="c" id="hp-c" class="form-check-input" checked disabled>
              <label class="form-check-label" for="hp-c">C</label>
            </div>
            <div class="form-check form-check-inline">
              <input type="radio" name="hp" value="d" id="hp-d" class="form-check-input" disabled>
              <label class="form-check-label" for="hp-d">D</label>
            </div>
            <div class="form-check form-check-inline">
              <input type="radio" name="hp" value="e" id="hp-e" class="form-check-input" disabled>
              <label class="form-check-label" for="hp-e">E</label>
            </div>
          </div>
        </div>
        <!-- /.form-group -->
      </div>
      <div class="col">
        <div class="form-group">
          <label for="radio" class="control-label">マジックポイント(MP)</label>
          <div>
            <div class="form-check form-check-inline">
              <input type="radio" name="mp" value="a" id="mp-a" class="form-check-input" disabled>
              <label class="form-check-label" for="mp-a">A</label>
            </div>
            <div class="form-check form-check-inline">
              <input type="radio" name="mp" value="b" id="mp-b" class="form-check-input" disabled>
              <label class="form-check-label" for="mp-b">B</label>
            </div>
            <div class="form-check form-check-inline">
              <input type="radio" name="mp" value="c" id="mp-c" class="form-check-input" checked disabled>
              <label class="form-check-label" for="mp-c">C</label>
            </div>
            <div class="form-check form-check-inline">
              <input type="radio" name="mp" value="d" id="mp-d" class="form-check-input" disabled>
              <label class="form-check-label" for="mp-d">D</label>
            </div>
            <div class="form-check form-check-inline">
              <input type="radio" name="mp" value="e" id="mp-e" class="form-check-input" disabled>
              <label class="form-check-label" for="mp-e">E</label>
            </div>
          </div>
        </div>
        <!-- /.form-group -->

      </div>
    </div>
    <div class="row">
      <div class="col">
        <div class="form-group">
          <label for="radio" class="control-label">力強さ(STR)</label>
          <div>
