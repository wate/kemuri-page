            <div class="form-check form-check-inline">
              <input type="radio" name="vit" value="e" id="vit-e" class="form-check-input" disabled>
              <label class="form-check-label" for="vit-e">E</label>
            </div>
          </div>
        </div>
        <!-- /.form-group -->
      </div>
    </div>
    <div class="row">
      <div class="col">
        <div class="form-group">
          <label for="radio" class="control-label">器用さ(DEX)</label>
          <div>
            <div class="form-check form-check-inline">
              <input type="radio" name="dex" value="a" id="dex-a" class="form-check-input" disabled>
              <label class="form-check-label" for="dex-a">A</label>
            </div>
            <div class="form-check form-check-inline">
